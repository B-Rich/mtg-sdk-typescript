{
	"compilerOptions": {
		"target": "es2015",
		"outDir": "out/",
		"module": "commonjs",
		"moduleResolution": "node",
		"forceConsistentCasingInFileNames": true,
		"noImplicitAny": true,
		"noImplicitReturns": true,
		"noImplicitThis": true,
		"noUnusedLocals": true,
		"removeComments": true,
		"sourceMap": true,
		"declaration": true
	}
}